  "paths": {
    "/api/weather/forecast": {
      "get": {
        "summary": "forecast weather",
        "description": "Fetch forecast weather data using  coordinate value, place name or zip code",
        "operationId": "forecast_weather",
        "parameters": [
          {
            "name": "lon",
            "in": "query",
            "required": false,
            "description": "Longitude coordinate(Note. lat and lon value has priority over both place and zip value). use the lat and lon for more accurate and reliable weather data",
            "schema": {
              "type": "string",
              "default": "0",
              "enum": null
            }
          },
          {
            "name": "lat",
            "in": "query",
            "required": false,
            "description": "Latitude coordinate(Note. lat and lon value has priority over both place and zip value). use the lat and lon for more accurate and reliable weather data",
            "schema": {
              "type": "string",
              "default": "0",
              "enum": null
            }
          },
          {
            "name": "place",
            "in": "query",
            "required": false,
            "description": "City name, state code (only for the US) and country code divided by comma like Zion,VA,US . Please use ISO 3166 country codes. Note. place value has priority over zip value",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "zip",
            "in": "query",
            "required": false,
            "description": "Zip code with optional country code(Note. Zip value will only be used if both lat,lon and place name not provided)",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "cnt",
            "in": "query",
            "required": false,
            "description": "Number of timestamps to retrieve (up to a maximum allowed by the API)",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "units",
            "in": "query",
            "required": false,
            "description": "Unit type (standard, metric, and imperial)",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "type",
            "in": "query",
            "required": false,
            "description": "Forcast Type",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "mode",
            "in": "query",
            "required": false,
            "description": "Mode type",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "lang",
            "in": "query",
            "required": false,
            "description": "Language type",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
