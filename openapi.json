    "/api/weather/us/alert": {
      "get": {
        "summary": "US weather alert data",
        "description": "US weather alert daily information for given region or area and other filtering parameter",
        "operationId": "us_weather_alert_data",
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "required": false,
            "description": "use comm(,) to separate multiple value",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "message_type",
            "in": "query",
            "required": false,
            "description": "Example value: alert,update,cancel",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "area",
            "in": "query",
            "required": false,
            "description": "example AM,AN",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "region",
            "in": "query",
            "required": false,
            "description": "Example value: AL,AT",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "zone",
            "in": "query",
            "required": false,
            "description": "Example>> OKZ929",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "region_type",
            "in": "query",
            "required": false,
            "description": "Example>> marine,land This value can't be used in combination with area or region",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "urgency",
            "in": "query",
            "required": false,
            "description": "Example value: Immediate,Expected",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "severity",
            "in": "query",
            "required": false,
            "description": "Example value: Extreme,Severe,Moderate",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "certainty",
            "in": "query",
            "required": false,
            "description": "Example value: Observed,Likely,Possible,Unlikely,Unknown",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "description": "Example value: 500",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
